  <script>  
 var res = 0;
    function parseResponse(json) {
     res = jQuery.parseJSON(json);
           
     if( res && res.exists == 'true'){
       update_score( res );

     };
     
   };
   
   function update_score(res){
      $('#starly_score').append( res.score );
       var score_tag = res.score_tag.toString();
       $('.rating div').addClass( 'stars_' + score_tag );
   };
   
    function update_comments(res){
      $('#starly_score').append( res.score );
       var score_tag = res.score_tag.toString();
       $('.rating div').addClass( 'stars_' + score_tag );
   };
    
      $.ajax({
       type: 'get',
       url:  BASE_URL + starly_settings.app_id + '/score',
       dataType: 'jsonp',
       success: function(data) {
         parseResponse(data);
        }
      });
     

		  
		 
  
</script>